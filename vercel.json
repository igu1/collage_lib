{
    "builds": [
      {
        "src": "core_library/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "core_library/wsgi.py"
      }
    ],
      "crons": [{
        "path": "/api/cron",
        "schedule": "0 9 * * *"
      }
    ]
    
  }